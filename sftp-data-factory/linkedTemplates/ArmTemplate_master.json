{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sftp-data-factory"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"Sftp1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Sftp1'"
		},
		"Sftp1_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "s-290b1fe907694ebc8.server.transfer.us-east-1.amazonaws.com"
		},
		"Sftp1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "triplink"
		},
		"sftp-trigger_properties_CopyPipeline_pez_parameters_sourceFolder": {
			"type": "string",
			"defaultValue": "@triggerBody().folderPath"
		},
		"sftp-trigger_properties_CopyPipeline_pez_parameters_sourceFile": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"sftp-trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/885a071e-fd10-4d2d-bc9e-bc7d064f37a8/resourceGroups/sftp-test-group/providers/Microsoft.Storage/storageAccounts/sftp1test1storage"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sftp-data-factory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"Sftp1_password": {
						"value": "[parameters('Sftp1_password')]"
					},
					"Sftp1_properties_typeProperties_host": {
						"value": "[parameters('Sftp1_properties_typeProperties_host')]"
					},
					"Sftp1_properties_typeProperties_userName": {
						"value": "[parameters('Sftp1_properties_typeProperties_userName')]"
					},
					"sftp-trigger_properties_CopyPipeline_pez_parameters_sourceFolder": {
						"value": "[parameters('sftp-trigger_properties_CopyPipeline_pez_parameters_sourceFolder')]"
					},
					"sftp-trigger_properties_CopyPipeline_pez_parameters_sourceFile": {
						"value": "[parameters('sftp-trigger_properties_CopyPipeline_pez_parameters_sourceFile')]"
					},
					"sftp-trigger_properties_typeProperties_scope": {
						"value": "[parameters('sftp-trigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}